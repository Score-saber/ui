<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
</head>
<body>
    <h1><%= playerName %>'s Recent scores</h1>
    <hr>
    <% scores.forEach(score => { %>
        <h2><%= score.songName %></h2>
        <p>by <%= score.songAuthorName %></p>
        <br>
    <% }) %>

</body>
</html>